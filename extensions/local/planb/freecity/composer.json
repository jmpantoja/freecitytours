{
    "name": "planb/freecity",
    "description": "Jerez Freecity Tours Extension for bolt",
    "type": "bolt-extension",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "jmpantoja",
            "email": "jmpantoja@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\PlanB\\FreeCity\\": "src"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\PlanB\\FreeCity\\FreeCityExtension",
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
